{
  "name": "spiralogic-oracle-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:beta": "tsc -p tsconfig.json --noEmitOnError",
    "build:prod": "npm run clean && esbuild src/server.ts --bundle --platform=node --target=node18 --outfile=dist/server.js --external:./node_modules/* --minify",
    "build:watch": "tsc -p tsconfig.json -w --preserveWatchOutput",
    "build:app": "tsc --project tsconfig.app.json",
    "build:all": "tsc --noEmitOnError false",
    "build:check": "tsc --project tsconfig.app.json --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test:agents": "node test-dual-agents.js",
    "test": "jest --passWithNoTests --coverage --detectOpenHandles",
    "start": "node dist/server.js",
    "dev": "nodemon",
    "seed:files": "tsx scripts/seed-sample-files.ts",
    "seed:sample": "tsx scripts/seed-sample-file.ts",
    "deepseek:setup": "./scripts/setup-deepseek-ollama.sh",
    "deepseek:chat": "tsx src/deepseek/terminal-chat.ts",
    "deepseek:serve": "tsx src/deepseek/api-server.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@aws-sdk/client-s3": "^3.884.0",
    "@grpc/grpc-js": "^1.9.0",
