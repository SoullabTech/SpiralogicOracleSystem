    "@langchain/core": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "@supabase/supabase-js": "^2.57.2",
    "@types/sqlite3": "^3.1.11",
    "axios": "^1.6.0",
    "better-sqlite3": "^8.7.0",
    "body-parser": "^1.20.0",
    "chalk": "^5.6.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "form-data": "^4.0.0",
    "formidable": "^3.5.0",
    "globby": "^14.0.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "langchain": "^0.3.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "ollama": "^0.5.17",
    "openai": "^4.20.0",
    "ora": "^8.2.0",
    "rate-limit-redis": "^4.2.2",
    "redis": "^4.6.0",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.0",
    "tsx": "^4.20.5",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@jest/globals": "^29.5.0",
    "@jest/types": "^29.5.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@types/better-sqlite3": "^7.6.0",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/formidable": "^3.4.0",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
