{
  "name": "@spiralogic/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:frontend": "next dev -p 3001",
    "dev:backend": "npm --prefix backend run dev",
    "dev:middleware": "nodemon frontend/server/index.js",
    "dev:all": "concurrently -n FRONTEND,BACKEND,MIDDLEWARE -c auto \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:middleware\"",
    "dev:full": "concurrently -n NEXT,EXPRESS,BACKEND -c auto \"npm run dev:frontend\" \"npm run dev:middleware\" \"npm run dev:backend\"",
    "clean:src": "bash -lc \"find backend/src -type f -name '._*' -delete && find backend/src -type f \\( -name '*.js' -o -name '*.js.map' \\) -delete\"",
    "clean:dist": "bash -lc \"rm -rf backend/dist\"",
    "clean": "npm run clean:dist && npm run clean:src",
    "build": "next build",
    "build:middleware": "echo 'Express middleware server is ready'",
    "start": "next start",
    "start:middleware": "node frontend/server/index.js",
    "start:all": "concurrently -n NEXT,EXPRESS \"npm start\" \"npm run start:middleware\"",
    "lint": "next lint",
    "seed": "npx tsx scripts/seed-sample-files.ts",
    "seed:force": "npx tsx scripts/seed-sample-files.ts --force",
    "test:ai": "npx tsx scripts/test-ai-integration.ts",
    "test:beta": "npx tsx scripts/test-beta-diagnostics.ts",
    "check:invites": "npx tsx scripts/check-beta-invites.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.61.0",
    "@aws-sdk/client-s3": "^3.879.0",
    "@elevenlabs/elevenlabs-js": "^2.13.0",
