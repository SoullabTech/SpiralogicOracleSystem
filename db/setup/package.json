{
  "name": "soullab-db-setup",
  "version": "1.0.0",
  "description": "Database setup and migration tools for Soullab Maya",
  "scripts": {
    "db:migrate": "bash ./run-migrations.sh",
    "db:migrate:dry": "echo 'Dry run - showing migration files:' && ls -la ../migrations/",
    "db:reset": "bash ./reset-database.sh",
    "db:seed": "bash ./seed-data.sh",
    "db:backup": "bash ./backup-database.sh",
    "db:test-rls": "node ./test-rls.js",
    "db:verify": "node ./verify-schema.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0"
  }
}