// --- File: oracle-frontend/vercel.json ---
{
  "version": 2,
  "name": "spiralogic-oracle",
  "builds": [
    {
      "src": "src/app/page.tsx",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/dashboard",
      "destination": "/dashboard/page.tsx"
    }
  ],
  "framework": "nextjs",
  "outputDirectory": "out",
  "cleanUrls": true,
  "trailingSlash": false
}
