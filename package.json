{
  "name": "oracle-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run dev:backend\" \"pnpm --filter frontend dev\"",
    "dev:backend": "nodemon --watch src --ext ts --exec tsx -r tsconfig-paths/register src/server.ts"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^3.0.2",
    "tsx": "^4.19.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
