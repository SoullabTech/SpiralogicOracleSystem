{
  "name": "spiralogic-oracle-system",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:frontend": "next dev -p 3001",
    "dev:backend": "npm --prefix backend run dev",
    "dev:all": "concurrently -n FRONTEND,BACKEND -c auto \"npm run dev:frontend\" \"npm run dev:backend\"",
    "clean:src": "bash -lc \"find backend/src -type f -name '._*' -delete && find backend/src -type f \\( -name '*.js' -o -name '*.js.map' \\) -delete\"",
    "clean:dist": "bash -lc \"rm -rf backend/dist\"",
    "clean": "npm run clean:dist && npm run clean:src",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bcryptjs": "^2.4.6",
    "@types/js-cookie": "^3.0.6",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "critters": "^0.0.23",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.542.0",
    "next": "^14.2.5",
    "openai": "^5.16.0",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^3.4.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^9.0.8",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.5",
    "husky": "^8.0.3",
    "typescript": "^5.9.2",
    "wait-on": "^8.0.4"
  }
}
