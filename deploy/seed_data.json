  "onboarding_flow": {
    "steps": [
      {
        "id": "welcome",
        "duration": "0-10s",
        "action": "show_archetype_introduction",
        "message": "I'm ARIA, your multi-perspective guide. I can channel different archetypal wisdom to support you."
      },
      {
        "id": "first_question",
        "duration": "10-30s",
        "action": "offer_starter_questions",
        "message": "What would you like to explore? Choose a question or ask your own."
      },
      {
        "id": "archetype_selection",
        "duration": "30-60s",
        "action": "suggest_archetype",
        "message": "Based on your interest, would you like the [suggested_archetype]'s perspective, or should I choose?"
      },
      {
        "id": "initial_response",
        "duration": "60-120s",
        "action": "provide_first_answer",
        "message": "[Response with exploration mode banner]"
      },
      {
        "id": "preference_learning",
        "duration": "120-300s",
        "action": "collect_feedback",
        "message": "Was this helpful? I'm learning your preferences to provide better guidance."
      },
      {
        "id": "transition",
        "duration": "300s+",
        "action": "switch_to_personalized",
        "message": "I'm getting to know you better. Responses will become more personalized as we continue."
      }
    ]
  },

  "performance_targets": {
    "cold_start": {
